{
  "name": "DnD_Ex2",
  "version": "1.0.0",
  "description": "2nd iteration of the DnD webapp in Typescript",
  "main": "lib/index.js",
  "types": "lib/index",
  "scripts": {
    "start": "npm run build && npm run server",
    "server": "concurrently \"gulp watch\" \"nodemon server/dist/src/index.js secretArg\" \"webpack --progress --colors --watch\"",
    "build": "npm run clean && gulp && webpack",
    "test": "mocha --reporter spec --compilers ts:ts-node/register test/helloWorld.test.ts",
    "clean": "rimraf client/dist client/typings server/dist",
    "prod": "cross-env ENV=production npm run clean && gulp && webpack --config client/config/webpack.prod.js && node server/dist/src/index.js secretArg"
  },
